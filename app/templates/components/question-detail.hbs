<div class="panel panel-info">
  <div class="panel-heading">
    <h1>{{question.mainQ}}</h1> by {{question.name}}<br>
  </div>
  <div class="panel-body">
    <p>{{question.notes}}</p>
  </div>
</div>

<h3>Answers</h3>
<ul>
  {{#each model.answers as |answer|}}
  <li>Name:{{answer.name}}</li>
  <li>Explanation:{{answer.explanation}}</li>
  {{/each}}
</ul>

{{new-answer saveAnswer="saveAnswer" question=question}}
